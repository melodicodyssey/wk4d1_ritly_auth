<h1>Sign In</h1>

<%= form_tag sessions_path do %>
			<%= label_tag :email %>
        	<%= text_field_tag :email %>
        	<%= label_tag :password %>
        	<%= password_field_tag :password %>
        	<%= submit_tag "Login" %>
        	<%= check_box_tag :remember%><p>Remember me</p>
        <% end %>
